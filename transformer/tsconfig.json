{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2019",
		"removeComments": true,
		"sourceMap": true,
		"stripInternal": true,
		"strict": true,
		"rootDir": ".",
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"include": [
		"./src/**/*.ts",
		"./src/*.ts",
		"./index.ts",
		"./generics.ts"
	]
}